<% provide(:title, "Главная") %>
<%= link_to t('link.new_proposal'), new_proposal_path %>
<h1>Главная</h1>
<ul>
  <li>
    <%= link_to @setting.set_name, setting_path(@setting) %>
  </li>
  <% @proposals.each do |proposal| %>
    <ul>
      <li>
        <%= link_to "#{proposal.prop_date_from.strftime("%d.%m.%Y")} #{proposal.prop_number} #{proposal.prop_applic_name}", proposal_path(proposal) %>
      </li>
      <% @solution_proposals.find_all { |x| x["proposal_id"] == proposal.id }.each do |solution_proposal| %>
        <% unless solution_proposal.nil? %>
          <ul>
            <li>
              <%= link_to "1.3 Решение по заявке о проведении сертификации от 
                          #{solution_proposal.solprop_date_from.strftime("%d.%m.%Y")} № 
                          #{solution_proposal.solprop_number}",
                          solution_proposal_path(solution_proposal, proposal_id: proposal.id) %>
            </li>
            <% @protocols.find_all { |x| x["solution_proposal_id"] == solution_proposal.id }.each do |protocol| %>
              <% unless protocol.nil? %>
                <ul>
                  <li>
                    <%= link_to "2.2.1 Протокол идентификации от #{protocol.prot_date_from.strftime("%d.%m.%Y")}",
                                protocol_path(protocol, solution_proposal_id: solution_proposal.id)%>
                  </li>
                </ul>
              <% end %>
            <% end %>
          </ul>
        <% end %>
      <% end %>
    </ul>
  <% end %>
</ul>
<br>
<br>
<br>
<%= link_to t('link.new_proposal'), new_proposal_path %>
